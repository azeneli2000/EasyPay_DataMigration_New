{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",


  "ConnectionStrings": {
    "db": "Server=(localdb)\\MSSQLLocalDB;Database=WorkOrderDb;Trusted_Connection=True;TrustServerCertificate=True;"
  },
  "ReportPath": "C:\\Imports\\Report.csv",

  "ImportConfigurations": {
    "WorkOrdersFromExcel": {
      "ExcelPath": "C:\\Imports\\WorkOrders.xlsx",
      "Mode": "Tvp", //Bulk/Tvp
      "Tvp": {
        "InsertSpName": "dbo.sp_WorkOrders_Insert",
        "ParameterName": "@WorkOrders",
        "TypeName": "dbo.WorkOrdersType",
        "ErrorSpName": "dbo.sp_WorkOrders_Error",
        "ErrorParameterName": "@WorkOrders_Error",
        "ErrorTypeName": "dbo.WorkOrdersType_Error"
      },
      "Bulk": {
        "ValidTable": "dbo.WorkOrders",
        "ErrorTable": "dbo.LoadingErrors"
      }
    },

    "ClientsFromExcel": {
      "ExcelPath": "C:\\Imports\\Clients.xlsx",
      "Mode": "Bulk",
      "Tvp": {
        "InsertSpName": "dbo.sp_Clients_Insert",
        "ParameterName": "@Clients",
        "TypeName": "dbo.ClientsType"
      },
      "Bulk": {
        "ValidTable": "dbo.Clients"
      }
    },

    "TechniciansFromExcel": {
      "ExcelPath": "C:\\Imports\\WorkOrders.xlsx",
      "Mode": "Bulk",
      "Tvp": {
        "InsertSpName": "dbo.sp_Technicians_Insert",
        "ParameterName": "@Technicians",
        "TypeName": "dbo.TechniciansType"
      },
      "Bulk": {
        "ValidTable": "dbo.Technicians"
      }
    }
  }
}
